1.
SELECT COUNT(*)
  FROM soccer_venue

2.
SELECT COUNT (DISTINCT team_id)
FROM player_mast

3. 
SELECT COUNT (goal_id)
FROM goal_details

4.
SELECT COUNT (*)
FROM match_mast
WHERE results = 'WIN'

5.
SELECT COUNT (*)
FROM match_mast
WHERE results = 'DRAW'

6.
SELECT play_date "Beginning Date"
FROM match_mast
WHERE match_no=1

